{
	"version": "0.1.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Mocha Test Moby Client",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"-u",
				"tdd",
				"--timeout",
				"999999",
				"--colors",
				"${workspaceFolder}/test/mobyclient-test"
			],
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}