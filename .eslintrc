{
  "extends": "airbnb/index",
  "rules": {
      "indent": [
          2,
          4
      ],
      "quotes": [
          2,
          "single"
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "semi": [
          2,
          "always"
      ],
      "comma-dangle": [
          2,
          "never"
      ]
  },
  "env": {
      "mocha": true,
      "node": true
  }
}